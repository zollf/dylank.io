type Project {
  id: ID!
  slug: String!
  title: String!
  description: String!
  image: String!
  url: String
  git: String
  tags: [Tag]
}

type Tag {
  id: ID!
  slug: String!
  title: String!
}

type TagInterface {
  tag: Tag!
  total: Int!
}

type Query {
  projects(tags: [String]): [Project]
  tags: [TagInterface]
}